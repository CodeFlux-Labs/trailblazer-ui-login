{
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-native/all",
      "plugin:prettier/recommended"
    ],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": ["warn"],
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "off"
    }
  }